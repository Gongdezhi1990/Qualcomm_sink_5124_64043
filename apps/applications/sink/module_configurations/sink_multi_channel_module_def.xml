                Desc="Sets a fixed gain applied to hardware in units of dB/60, only used when the volume scaling method is set to DSP"
                Range="Min=-7200,Max=0"
                Type="int"
                Size="16"
                ConfigGroupPath="./[@ShortId='audio']/[@ShortId='output']"
