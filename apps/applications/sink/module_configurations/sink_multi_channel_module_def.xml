                DefaultValue="DSP">
                <BuildVariant HwVariant="H13672v2 R13568v4 R13568v4_A05" SwVariant="All" Value="Hardware"/>
                <enum key="Hardware"
                      value="0"/>
                <enum key="DSP"
                      value="1"/>
                <enum key="Hybrid"
                      value="2"/>
            </ConfigItem>
            <ConfigItem
